param "$mfactor"
units = "", desc = "Multiplier (Verilog-A $mfactor)", flags = ParameterFlags(PARA_KIND_INST)
param "r"
units = "Ohm", desc = "Resistance", flags = ParameterFlags(PARA_KIND_INST)
param "has_noise"
units = "", desc = "Noise switch", flags = ParameterFlags(PARA_TY_INT)

2 terminals
node "A" units = "V", runits = "A"
node "B" units = "V", runits = "A"
jacobian (A, A) JacobianFlags(JACOBIAN_ENTRY_RESIST | JACOBIAN_ENTRY_REACT_CONST)
jacobian (A, B) JacobianFlags(JACOBIAN_ENTRY_RESIST | JACOBIAN_ENTRY_REACT_CONST)
jacobian (B, A) JacobianFlags(JACOBIAN_ENTRY_RESIST | JACOBIAN_ENTRY_REACT_CONST)
jacobian (B, B) JacobianFlags(JACOBIAN_ENTRY_RESIST | JACOBIAN_ENTRY_REACT_CONST)
noise "thermal" (A, B)
0 states
has bound_step false
